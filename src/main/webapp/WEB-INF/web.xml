<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Servlets.CurrenciesServlet</servlet-name>
    <servlet-class>Servlets.CurrenciesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlets.CurrenciesServlet</servlet-name>
    <url-pattern>/currencies</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Servlets.CurrencyServlet</servlet-name>
    <servlet-class>Servlets.CurrencyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlets.CurrencyServlet</servlet-name>
    <url-pattern>/currency/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Servlets.ExchangeRatesServlet</servlet-name>
    <servlet-class>Servlets.ExchangeRatesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlets.ExchangeRatesServlet</servlet-name>
    <url-pattern>/exchangeRates</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Servlets.ExchangeRateServlet</servlet-name>
    <servlet-class>Servlets.ExchangeRateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlets.ExchangeRateServlet</servlet-name>
    <url-pattern>/exchangeRate/*</url-pattern>
  </servlet-mapping>
</web-app>
